{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/node",
        "config": {
          "maxLambdaSize": "50mb"
        }
      },
      {
        "src": "frontend/build/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/frontend/build/(.*)",
        "dest": "/frontend/build/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/build/index.html"
      }
    ]
  }
  